        "colorette": "^2.0.19",
        "eslint": "^8.25.0",
        "prettier": "^2.7.1",
        "puppeteer": "^18.2.1",
        "sass": "^1.55.0",
        "serve": "^14.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.6.4",
        "vite": "^3.1.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.2.0",
